page{
    background: #f1f1f1;
}
view,button{ 
    display: block;
    overflow: initial;
}
.setTab-box-view{ 
    position: fixed;
} 
.common-setTab-sec{
    width: 74%;
    padding: 0rpx 13%;
    top: 0rpx;
}
.myearning-history {
    width: 100%;
    height: auto; 
    padding: 0rpx;
    margin: 20rpx 0rpx;
    display: block;
    box-shadow: none;
    background: none;
}
.myearning-history .commission{
    margin-top: 12rpx;
    width: 92%;
    padding: 0rpx 4%;
    background: white;
}
.myearning-history .commission .child .user{
    border-top: transparent;
    padding: 0rpx;
}
.myearning-history .commission .child{ 
    border-bottom: 1rpx solid #f1f1f1;
}
.myearning-history .commission .child:nth-last-child(1){
    border: transparent;
}
.myearning-history .commission .child .product{
    width: 100%;
    padding: 30rpx 0rpx;
}  