page{
    background: #f1f1f1;
}
view,button{ 
    display: block;
    overflow: initial;
}
.common-setTab-sec{
    width: 74%;
    padding: 0rpx 13%;
    top: 0rpx;
}
.myearning-history {
    width: 92%;
    height: auto;
    padding: 0rpx 4%;
    margin: 20rpx 0rpx;
    display: block;
    box-shadow: none;
}
.myearning-history .commission .child{ 
    border-bottom: 1rpx solid #f1f1f1;
}
.myearning-history .commission .child:nth-last-child(1){
    border: transparent;
}
.myearning-history .commission .child .product{
    width: 100%;
    padding: 30rpx 0rpx;
}
.myearning-history .commission .child .product .content .name{
    margin-right: 100rpx;
}